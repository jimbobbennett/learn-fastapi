{
  "sdkName": "Exciting-Soda",
  "specFilePath": "spec.json",
  "languages": [
    "python"
  ],
  "createDocs": false,
  "customizations": {
    "license": "MIT",
    "retry": {
      "enabled": true,
      "maxAttempts": 3,
      "retryDelay": 150
    },
    "inferServiceNames": true
  },
  "languageOptions": {
    "python": {
      "pypiPackageName": "exciting-soda",
      "alwaysInitializeOptionals": true,
      "githubRepoName": "learn-fastapi-sdk-python",
      "sdkVersion": "0.0.1"
    }
  },
  "publishing": {
    "githubOrg": "jimbobbennett"
  },
  "validationsToIgnore": [
    {
      "code": "oas3-server-trailing-slash",
      "always": true
    }
  ]
}